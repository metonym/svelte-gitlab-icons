{
  "name": "svelte-gitlab-icons",
  "version": "0.3.1",
  "license": "MIT",
  "description": "GitLab SVG icons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "rollup -cw",
    "predeploy": "rollup -c",
    "deploy": "gh-pages -d dist",
    "prepack": "svelvg glob=@gitlab/svgs/dist/sprite_icons iconIndex",
    "test": "svelte-check --workspace test"
  },
  "devDependencies": {
    "@gitlab/svgs": "2.12.0",
    "gh-pages": "^3.2.3",
    "rollup": "^2.72.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-readme": "^3.6.3",
    "svelvg": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-gitlab-icons.git"
  },
  "homepage": "https://github.com/metonym/svelte-gitlab-icons",
  "bugs": "https://github.com/metonym/svelte-gitlab-icons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "gitlab",
    "@gitlab/svgs",
    "icon",
    "svg"
  ],
  "files": [
    "lib"
  ],
  "type": "module"
}
